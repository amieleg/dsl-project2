  <volumes
      xsi:type="bouldering:Rectangle"
      depth="10"
      width="400"
      height="500">
    <position x="0" y="0"/>
    <holds id="0001" shape="52" start_hold="1" end_hold="false" rotation="45">
      <position x="50" y="50"/><colours>Blue</colours>
    </holds>
    <holds id="0002" shape="51" start_hold="0" end_hold="false" rotation="0">
