    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bouldering="http://www.example.org/bouldering"
    xsi:schemaLocation="http://www.example.org/bouldering ../model/bouldering.ecore"
    id="BigWall">
  
  <routes
      id="BlueSlabDream"
      grade="6B"
      holds="#0001 #0003 #0005 #0007 #0009 #0011 #0018">
